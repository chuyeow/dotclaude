{
  "mcpServers": {
    "chrome-devtools": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "chrome-devtools-mcp@latest"
      ],
      "env": {}
    },
    "context7": {
      "type": "sse",
      "url": "https://mcp.context7.com/sse"
    },
    "private-journal": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "github:obra/private-journal-mcp"
      ]
    },
    "google-calendar-zapier": {
      "type": "http",
      "url": "https://mcp.zapier.com/api/mcp/mcp",
      "headers": {
        "Authorization": "Bearer get-this-from-zapier at https://mcp.zapier.com/mcp"
      }
    },
    "cloudflare-browser-rendering": {
      "type": "sse",
      "url": "https://browser.mcp.cloudflare.com/sse"
    },
    "cloudflare-container-sandbox": {
      "type": "sse",
      "url": "https://containers.mcp.cloudflare.com/sse"
    }
  }
}
